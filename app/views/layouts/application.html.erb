<!DOCTYPE html>
<html>
	<head>
		<title>Promobug</title>
	  	<%= stylesheet_link_tag :all %>
	  	<%= javascript_include_tag :defaults %>
	  	<%= javascript_include_tag "prototype", "jquery-2.0.0.min" %>
	  	<script>
		 	jQuery.noConflict();
	  	</script>
	  	<%= javascript_include_tag "rails", "application", "controls", "effects", "dragdrop" %>
	  	<%= csrf_meta_tag %>
	</head>
	
	<body>
		<div id="main_bar">
			<%= render :partial=> "layouts/header" if params[:controller] != "devise/sessions"%>
		</div>
		<div id="main_sheet"> 
			<div style="height:50px;"></div>
			<div id="main_content">
				<%= yield %>
			</div>
		</div>
		<div id="bottom_bar">
			<p>Desenvolvido por Guilherme Cruz</p>
		</div>
	</body>
</html>
